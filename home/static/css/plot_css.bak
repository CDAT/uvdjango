@CHARSET "ISO-8859-1";
html, body
{
    height: 100%;
}

body
{
    font: 14px 'Lucida Sans Unicode', 'Trebuchet MS', Arial, Helvetica;    
    margin: 0;    
}

#coord_div input
{
    width: 40px; 
    border: 1px solid #ccc;
}
#coord_div label
{
	width:8px;
	color: #666;
    font: 14px 'Lucida Sans Unicode', 'Trebuchet MS', Arial, Helvetica;
}
h1
{
    text-shadow: 0 1px 0 rgba(255, 255, 255, .7), 0px 2px 0 rgba(0, 0, 0, .5);
    text-align: left;
    color: #666;
    margin: 0 0 10px 0;
    letter-spacing: 4px;
    font: normal 26px/1 Verdana, Helvetica;
    position: relative;
    top: 5px;
}
#top_link a
{
    text-align: right;
    position: absolute;
    right:10px;
    top: 10px;
    color: #999;
}
#footer a
{
    display: block;
    text-align: center;
    position: relative;
    right:10px;
    top: 10px;
    color: #999;
}

#flip,#flip1,#flip2
{
padding:2px;
text-align:left;
background-color:#e5eecc;
border:solid 1px #c3c3c3;
}
#panel,#dataset_div,#coord_div
{
padding:2px;
border:solid 1px #c3c3c3;
}
